{
  "tasks": {
    "start": "deno run -A --watch=routes,islands,components,utils,middleware main.ts",
    "build": "deno task build:vite && deno bundle main.ts",
    "build:vite": "deno run -A npm:vite build",
    "dev": "deno run -A main.ts",
    "test": "deno test --allow-all"
  },
  "imports": {
    "fresh": "https://deno.land/x/fresh@1.5.0/mod.ts",
    "fresh/runtime": "https://deno.land/x/fresh@1.5.0/runtime.ts",
    "@preact/signals": "https://esm.sh/@preact/signals@1.2.2",
    "@preact/preset-vite": "npm:@preact/preset-vite@4.0.0",
    "preact": "https://esm.sh/preact@10.19.5",
    "preact/": "https://esm.sh/preact@10.19.5/",
    "@supabase/supabase-js": "https://esm.sh/@supabase/supabase-js@2.38.8",
    "std/": "https://deno.land/std@0.208.0/",
    "dotenv": "https://deno.land/std@0.208.0/dotenv/mod.ts",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/core": "https://esm.sh/twind"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
